<p id="notice"><%= notice %></p>

<h1>Allocation Tests</h1>

<table>
  <thead>
    <tr>
      <th>Test</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @allocation_tests.each do |allocation_test| %>
      <tr>
        <td><%= allocation_test.test_id %></td>
        <td><%= allocation_test.user_id %></td>
        <td><%= link_to 'Show', allocation_test %></td>
        <td><%= link_to 'Edit', edit_allocation_test_path(allocation_test) %></td>
        <td><%= link_to 'Destroy', allocation_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Allocation Test', new_allocation_test_path %>
