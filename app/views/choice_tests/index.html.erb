<p id="notice"><%= notice %></p>

<h1>Choice Tests</h1>

<table>
  <thead>
    <tr>
      <th>Test</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @choice_tests.each do |choice_test| %>
      <tr>
        <td><%= choice_test.test_id %></td>
        <td><%= choice_test.user_id %></td>
        <td><%= link_to 'Show', choice_test %></td>
        <td><%= link_to 'Edit', edit_choice_test_path(choice_test) %></td>
        <td><%= link_to 'Destroy', choice_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Choice Test', new_choice_test_path %>
